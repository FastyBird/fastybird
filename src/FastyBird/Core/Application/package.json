{
  "name": "@fastybird/application",
  "version": "1.0.0-dev.24",
  "type": "module",
  "description": "FastyBird IoT application user interface",
  "keywords": [
    "fastybird",
    "fb",
    "ui",
    "interface",
    "vue"
  ],
  "homepage": "https://www.fastybird.com",
  "bugs": "https://github.com/FastyBird/fastybird/issues",
  "license": "Apache-2.0",
