    "element-plus": "^2.8",
    "lodash.get": "^4.4",
    "md5": "^2.3",
    "nprogress": "^0.2",
    "pinia": "^2.2",
    "unocss": "^0.64",
    "vue": "^3.5",
    "vue-i18n": "^10.0",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.4"
