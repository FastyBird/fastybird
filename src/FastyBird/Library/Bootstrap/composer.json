{
  "name": "fastybird/bootstrap-library",
  "type": "library",
  "description": "FastyBird IoT application bootstrap",
  "keywords": [
    "bootstrap",
    "php",
    "nette",
    "configuration",
    "fb",
    "fastybird"
  ],
  "homepage": "https://www.fastybird.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "FastyBird s.r.o.",
      "email": "code@fastybird.com",
      "homepage": "https://www.fastybird.com"
    },
    {
      "name": "Adam Kadlec",
      "email": "adam.kadlec@fastybird.com"
    }
  ],
  "support": {
    "email": "code@fastybird.com",
    "issues": "https://github.com/FastyBird/fastybird/issues",
    "source": "https://github.com/FastyBird/bootstrap-library"
  },
  "require": {
    "php": ">=8.1.0",
    "contributte/console": "^0.9",
    "contributte/monolog": "^0.5",
    "cweagans/composer-patches": "^1.7",
    "mangoweb/monolog-tracy-handler": "^1.0",
    "nette/bootstrap": "^3.1",
    "nette/di": "^3.0",
    "nette/utils": "^3.2",
    "sentry/sdk": "^3.1",
    "symfony/monolog-bridge": "^6.1",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "brianium/paratest": "^6.6",
    "dg/bypass-finals": "^1.4",
    "doctrine/orm": "^2.8",
    "infection/infection": "^0.26",
    "latte/latte": "^3.0",
    "orisai/coding-standard": "^3.2",
    "pds/skeleton": "^1.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.1",
    "phpstan/phpstan-phpunit": "^1.1",
    "phpstan/phpstan-strict-rules": "^1.4",
    "phpunit/phpunit": "^9.5",
    "staabm/annotate-pull-request-from-checkstyle": "^1.8",
    "tracy/tracy": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "FastyBird\\Library\\Bootstrap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FastyBird\\Library\\Bootstrap\\Tests\\Cases\\Unit\\": "tests/cases/unit"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "bin": [
    "bin/fb-console",
    "bin/fb-console.php",
    "bin/fb-supervisor",
    "bin/fb-supervisor.php"
  ],
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "stable"
}
