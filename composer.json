    "extra": {
        "enable-patching": false,
        "patches": {
            "nettrine/orm": {
                "Enable connection overrides": "https://raw.githubusercontent.com/FastyBird/libraries-patches/master/nettrine.orm.mangerRegistry.diff"
            },
            "doctrine/orm": {
                "Bug: Ramsey uuid not working - Part 1": "https://raw.githubusercontent.com/FastyBird/libraries-patches/master/doctrine.orm.uuid.1.diff",
                "Bug: Ramsey uuid not working - Part 2": "https://raw.githubusercontent.com/FastyBird/libraries-patches/master/doctrine.orm.uuid.2.diff"
            },
            "ramsey/uuid-doctrine": {
                "Bug: Ramsey uuid conversion fallback": "https://raw.githubusercontent.com/FastyBird/libraries-patches/master/ramsey.uuid.doctrine.diff"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    },
    "replace": {
        "fastybird/apikey-plugin": "self.version",
        "fastybird/devices-module": "self.version",
        "fastybird/fb-mqtt-connector": "self.version",
        "fastybird/homekit-connector": "self.version",
        "fastybird/metadata-library": "self.version",
        "fastybird/modbus-connector": "self.version",
        "fastybird/redisdb-devices-module-bridge": "self.version",
        "fastybird/redisdb-plugin": "self.version",
        "fastybird/redisdb-ws-exchange-bridge": "self.version",
        "fastybird/shelly-connector": "self.version",
        "fastybird/tuya-connector": "self.version",
        "fastybird/web-server-plugin": "self.version",
        "fastybird/ws-exchange-plugin": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
